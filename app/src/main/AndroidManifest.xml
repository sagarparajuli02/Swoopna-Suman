<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sagar.swoopnasuman">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".AppController"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Splash"
            android:label="@string/app_name"
            android:theme="@style/CustomTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".VideoActivity" />
        <activity android:name=".PhotoGallery" />
        <activity android:name=".NewsDetails" />
        <activity android:name=".Playlist" />
        <activity android:name=".FullScreenDemoActivity" />
        <activity android:name=".TenderActivity" />
        <activity android:name=".LyricsDetails" />
        <activity android:name=".Facts" />
        <activity android:name=".Disclaminar"></activity>
    </application>

</manifest>